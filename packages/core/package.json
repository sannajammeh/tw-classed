{
  "name": "@tw-classed/core",
  "version": "1.2.0-canary.2",
  "description": "A Stitches & Styled-Components inspired library to create reusable Tailwind react components",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "rimraf ./dist && unbuild",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "cd ../../ && turbo run build test --filter=@tw-classed/core"
  },
  "keywords": [],
  "author": "Sanna Jammeh",
  "license": "MIT",
  "homepage": "https://github.com/sannajammeh/tw-classed",
  "repository": {
    "type": "git",
    "url": "https://github.com/sannajammeh/tw-classed"
  },
  "devDependencies": {
    "@jest/core": "^29.3.1",
    "@types/jest": "^29.2.2",
    "@vitest/ui": "^0.25.1",
    "bunchee": "^2.1.6",
    "c8": "^7.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vitest": "^0.25.1"
  }
}
