{
  "name": "@tw-classed/solid",
  "version": "1.5.1",
  "description": "A Stitches & Styled-Components inspired library to create reusable Tailwind Solid components",
  "sideEffects": false,
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build -w",
    "build": "vite build",
    "test-x": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit --project ./tsconfig.test.json",
    "prepublishOnly": "cd ../../ && turbo run build test --filter=@tw-classed/solid"
  },
  "keywords": [],
  "author": "Sanna Jammeh",
  "license": "MIT",
  "homepage": "https://github.com/sannajammeh/tw-classed",
  "repository": {
    "type": "git",
    "url": "https://github.com/sannajammeh/tw-classed"
  },
  "dependencies": {
    "@tw-classed/core": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "jsdom": "^22.1.0",
    "solid-js": "^1.7.8",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^3.4.0",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.34.0"
  }
}
